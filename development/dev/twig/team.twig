{# index.twig #}
{% extends "_layout.twig" %}

	{# Page Data #}
	{% set page = "team" %}
	{% set pageID = "teamPage" %}

	{# Metadata #}
	{% set title = "Website Title" %}
	{% set subTitle = "Page Title" %}
	{% set date = "April 12th 2020" %}
	{% set description = "Description" %}
	{% set thumbnail = "img/thumbnail.jpg" %}

{% block content %}

<header id="top">
	{% include './includes/template/_navigation.twig' %}
	<div class="homepageHeader">
		<div class="header-left">
			{% include './includes/cards/_news.twig' %}
		</div>
		<div class="header-right">
			<div class="header-right-top">
				{% include './includes/cards/_team-slider.twig' %}
			</div>
			<div class="header-right-bottom">
				{% include './includes/cards/_page-store.twig' %}
				{% include './includes/cards/_page.twig' %}
			</div>
		</div>
	</div>
	{% include './includes/template/_sponsors.twig' %}
</header>

<section class="demo">

	<!-- News Cards -->
	<div class="row">
		<div class="col c6">
			{% include './includes/cards/_news.twig' %}
		</div>
		<div class="col c6">
			{% include './includes/cards/_news.twig' %}
		</div>
	</div>

	<!-- Player Cards -->
	<div class="row">
		<div class="col c4">
			{% include './includes/cards/_player.twig' %}
		</div>
		<div class="col c4">
			{% include './includes/cards/_player.twig' %}
		</div>
		<div class="col c4">
			{% include './includes/cards/_player.twig' %}
		</div>
	</div>

	<!-- Livestream Cards -->
	<div class="row">
		<div class="col c4">
			{% include './includes/cards/_livestream.twig' %}
		</div>
		<div class="col c4">
			{% include './includes/cards/_livestream.twig' %}
		</div>
		<div class="col c4">
			{% include './includes/cards/_livestream.twig' %}
		</div>
	</div>

	<!-- Store Cards -->
	<div class="row">
		<div class="col c4">
			{% include './includes/cards/_store.twig' %}
		</div>
		<div class="col c4">
			{% include './includes/cards/_store.twig' %}
		</div>
		<div class="col c4">
			{% include './includes/cards/_store.twig' %}
		</div>
	</div>

</section>

{% endblock %}

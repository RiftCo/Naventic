<!DOCTYPE html>
<html lang="en">
{% include '../includes/template/_head.twig' %}
<body class="{{ page }}">

	<!-- Navigation -->
	{% include '../includes/template/_navigation.twig' %}

	<h3 id="example1">Example: </h3>

	<!-- Content -->
	{% block content %}{% endblock %}

	<!-- Footer -->
	<footer>{% include '../includes/template/_footer.twig' %}</footer>

	<script src="https://code.jquery.com/jquery-3.0.0.min.js" type="text/javascript"></script>
	<script src="https://cdn.jsdelivr.net/jquery.typeit/4.4.0/typeit.min.js" type="text/javascript"></script>

	<script>
		$('#example1').typeIt({
			strings: ["This is a great string.", "But here is a better one."],
			speed: 50,
			breakLines: false,
			autoStart: false
		});
	</script>

</body>
</html>
